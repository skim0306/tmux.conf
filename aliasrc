

alias tmuxatt='tmux attach -t'

alias ..='cd ../'
alias ...='cd ../../'
alias ....='cd ../../../'
alias .....='cd ../../../../'

alias gits='git status'
alias gitf='git fetch'
alias gitp='git pull'

alias edit='emacs -nw'
alias egrep='egrep --color=auto'
alias ll='ls -lah --color=auto'

alias mkdir='mkdir -pv'
